{
  "name": "monorepo-gitmodules",
  "private": true,
  "packageManager": "pnpm@7.33.5",
  "version": "0.1.0",
  "description": "monorepo gitmodules micro-frontend",
  "keywords": [
    "monorepo",
    "wujie",
    "gitmodules",
    "micro-frontend"
  ],
  "author": "zouzhixiang",
  "license": "MIT",
  "scripts": {
